{
  "java.configuration.updateBuildConfiguration": "automatic",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "bin/": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },

  "java.test.config": {
    "name": "testlibs",
      "env": {
        "path": "{$workspaceFolder}\\build\\tmp\\jniExtractDir"
      }
  } ,

  "java.test.defaultConfig": "testlibs",
  
  "liveshare.languages.allowGuestCommandControl": true
}
